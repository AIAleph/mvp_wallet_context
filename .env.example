# Docker Compose environment (copy to .env and adjust as needed)
REDIS_HOST_PORT=6380
CLICKHOUSE_DB=wallets
CLICKHOUSE_USER=default
# Used by the ClickHouse container (server)
CLICKHOUSE_PASSWORD=

# Application config (optional convenience)
# Preferred single DSN for app (overrides parts below if set)
# Example: http://user:pass@localhost:8123/wallets
CLICKHOUSE_DSN=

# Or provide parts to build DSN
CLICKHOUSE_URL=http://localhost:8123
# Used by the app; distinct from CLICKHOUSE_PASSWORD
CLICKHOUSE_PASS=
REDIS_URL=redis://localhost:6380

# API health debug (enables /healthz details)
HEALTH_DEBUG=
# Health probe timeout in milliseconds (default 3000)
HEALTH_PING_TIMEOUT_MS=
# Health cache TTL in milliseconds (default 5000)
HEALTH_CACHE_TTL_MS=
# Health endpoints rate limit (requests per second; 0 disables)
HEALTH_RATE_LIMIT_RPS=
